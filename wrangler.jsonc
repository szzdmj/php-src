{
  "name": "php-src-workers",
  "main": "./worker.mjs",
  "assets": {
    "directory": "./cloudflare"
  },
  "build": {
    "upload": {
      "format": "modules",
      "dir": "./cloudflare",
      "rules": [
        { "type": "ESModule", "globs": ["**/*.mjs"] },
        { "type": "Data", "globs": ["**/*.wasm"] }
      ]
    }
  }
}
