{
  "name": "php-src-workers",
  "main": "./worker.mjs",
  "compatibility_date": "2025-08-03",
  "build": {
    "upload": {
      "format": "modules",
      "dir": "./cloudflare",
      "rules": [
        { "type": "ESModule", "globs": ["**/*.mjs"] },
        { "type": "WebAssembly", "globs": ["**/*.wasm"] }
      ]
    }
  },
  "observability": {
    "logs": {
      "enabled": true
    }
  }
}
